$('#<%= dom_id(@post) %>').find('.upvote a img').attr({
    src: '/assets/unvote.png',
    alt: 'Unvote'
});
$('#<%= dom_id(@post) %>').find('.upvote a').attr('data-method', 'delete');
$('#<%= dom_id(@post) %>').find('.upvote a').attr('href', 'posts/<%= @post.id %>/unvote');
$('#<%= dom_id(@post) %>').find('.net_val').text('<%= @post.votes.length %>');