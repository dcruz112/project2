$('#<%= dom_id(@post) %>').find('.upvote a img').attr({
    src: '/assets/vote.png',
    alt: 'Vote'
});
$('#<%= dom_id(@post) %>').find('.upvote a').attr('data-method', 'put');
$('#<%= dom_id(@post) %>').find('.upvote a').attr('href', 'posts/<%= @post.id %>/upvote');
$('#<%= dom_id(@post) %>').find('.net_val').text('<%= @post.votes.length %>');